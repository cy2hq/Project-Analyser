/* Fluid style classes for pages */
select A.PNLNAME, A.FFSTYLEDESKTOP, A.FFSTYLEPHONE, A.FFSTYLETABLET, A.FFSTYLEMEDIUM, A.FFSTYLEEXLARGE 
from PSPNLDEFN A WHERE 
(A.FFSTYLEDESKTOP <> ' ' OR 
A.FFSTYLEPHONE  <> ' ' OR A.FFSTYLETABLET  <> ' ' OR A.FFSTYLEMEDIUM  <> ' ' OR  A.FFSTYLEEXLARGE  <> ' ')
AND EXISTS(
SELECT 'X' FROM 
PSPROJECTITEM B
  WHERE  B.OBJECTTYPE = 5
     AND A.PNLNAME = B.OBJECTVALUE1
        AND B.PROJECTNAME = 'CY2_LOGIN_001');

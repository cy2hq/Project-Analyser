/* Fluid style classes for page fields */
SELECT A.PNLNAME, A.RECNAME, A.FIELDNAME, A.FIELDNUM, B.FFSTYLELONG FROM PSPNLFIELD A, PSPNLFIELDEXT B WHERE B.FFSTYLELONG NOT LIKE ' | | | | ' AND B.FFSTYLELONG NOT LIKE ' ' AND A.PNLNAME = B.PNLNAME AND A.PNLFLDID = B.PNLFLDID AND EXISTS( SELECT 'X' FROM PSPROJECTITEM C WHERE C.OBJECTTYPE = 5 AND A.PNLNAME = C.OBJECTVALUE1 AND C.PROJECTNAME = : 1 );
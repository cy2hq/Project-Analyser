/* MISSING RELATED LANGUAGE RECORDS IN PROJECT  */
     SELECT B.RECNAME, B.RELLANGRECNAME
  FROM PSPROJECTITEM A, PSRECDEFN B
  WHERE  A.OBJECTTYPE = 0
     AND A.OBJECTVALUE1 = B.RECNAME
     AND A.PROJECTNAME = 'CY2_AD_APPL_001' 
     AND B.RELLANGRECNAME <> ' '
     AND NOT EXISTS(SELECT 'X' FROM 
  PSRECDEFN C, PSPROJECTITEM D
  WHERE D.OBJECTTYPE = 0
     AND C.RECNAME = D.OBJECTVALUE1
     AND C.RECNAME = B.RELLANGRECNAME
     AND A.PROJECTNAME = D.PROJECTNAME)

/* MISSING PROMPT RECORDS IN PROJECT  */
     SELECT B.RECNAME, B.EDITTABLE
  FROM PSPROJECTITEM A, PSRECFIELD B, PSRECDEFN E
  WHERE  A.OBJECTTYPE = 0
     AND A.OBJECTVALUE1 = B.RECNAME
     AND A.PROJECTNAME = 'CY2_AD_APPL_001' 
     AND B.EDITTABLE <> ' '
     AND B.EDITTABLE = E.RECNAME
     AND E.LASTUPDOPRID <> 'PPLSOFT'
     AND NOT EXISTS(SELECT 'X' FROM 
  PSRECDEFN C, PSPROJECTITEM D
  WHERE D.OBJECTTYPE = 0
     AND C.RECNAME = D.OBJECTVALUE1
     AND C.RECNAME = B.EDITTABLE
     AND A.PROJECTNAME = D.PROJECTNAME)
     
/* MISSING DEFAULT RECORDS FOR FIELDS IN PROJECT  */
  SELECT B.RECNAME, B.DEFRECNAME
  FROM PSPROJECTITEM A, PSRECFIELD B, PSRECDEFN E
  WHERE  A.OBJECTTYPE = 0
     AND A.OBJECTVALUE1 = B.RECNAME
     AND A.PROJECTNAME = 'CY2_AD_APPL_001' 
     AND B.DEFRECNAME <> ' '
     AND B.DEFRECNAME = E.RECNAME
     AND E.LASTUPDOPRID <> 'PPLSOFT'
     AND NOT EXISTS(SELECT 'X' FROM 
  PSRECDEFN C, PSPROJECTITEM D
  WHERE D.OBJECTTYPE = 0
     AND C.RECNAME = D.OBJECTVALUE1
     AND C.RECNAME = B.DEFRECNAME
     AND A.PROJECTNAME = D.PROJECTNAME)
       
/* FIELDS IN THE PROJECT THAT ARE NOT IN PAGES IN THE PROJECT */
SELECT A.PROJECTNAME, B.FIELDNAME
  FROM PSPROJECTITEM A, PSDBFIELD B
  WHERE  A.OBJECTTYPE = 2
     AND A.OBJECTVALUE1 = B.FIELDNAME
     AND A.OBJECTID2 = 0
     AND A.PROJECTNAME = 'CY2_AD_APPL_001' 
     AND NOT EXISTS(SELECT 'X' FROM 
  PSPNLFIELD C, PSPROJECTITEM D
  WHERE  D.OBJECTTYPE = 5
     AND C.PNLNAME = D.OBJECTVALUE1
     AND B.FIELDNAME = C.FIELDNAME 
     AND A.PROJECTNAME = D.PROJECTNAME)

/* Panel FIELDS WHERE fields not in the project */
SELECT C.PNLNAME, C.FIELDNAME FROM 
  PSPNLFIELD C, PSPROJECTITEM D
  WHERE  D.OBJECTTYPE = 5
     AND C.PNLNAME = D.OBJECTVALUE1 
     AND C.FIELDNAME <> ' '
        AND D.PROJECTNAME = 'CY2_AD_APPL_001' 
     AND NOT EXISTS(SELECT 'X' 
  FROM PSPROJECTITEM A, PSDBFIELD B
  WHERE A.OBJECTTYPE = 2
     AND A.OBJECTVALUE1 = B.FIELDNAME
     AND A.OBJECTID2 = 0
     AND B.FIELDNAME = C.FIELDNAME
    AND A.PROJECTNAME = D.PROJECTNAME)

     /* FIELDS IN THE PROJECT THAT ARE NOT IN RECORDS IN THE PROJECT */
     SELECT A.PROJECTNAME, B.FIELDNAME
  FROM PSPROJECTITEM A, PSDBFIELD B
  WHERE  A.OBJECTTYPE = 2
     AND A.OBJECTVALUE1 = B.FIELDNAME
     AND A.OBJECTID2 = 0
     AND A.PROJECTNAME = 'CY2_AD_APPL_001' 
     AND NOT EXISTS(SELECT 'X' FROM 
  PSRECFIELD C, PSPROJECTITEM D
  WHERE  D.OBJECTTYPE = 0
     AND C.SUBRECORD = 'N'
     AND C.RECNAME = D.OBJECTVALUE1
     AND B.FIELDNAME = C.FIELDNAME
     AND A.PROJECTNAME = D.PROJECTNAME)
     
     /* RECORD FIELDS IN THE PROJECT THAT ARE NOT AS FIELDS IN THE PROJECT */
     SELECT DISTINCT A.PROJECTNAME, B.FIELDNAME
  FROM PSPROJECTITEM A, PSRECFIELD B
  WHERE  A.OBJECTTYPE = 0
     AND A.OBJECTVALUE1 = B.RECNAME
     AND A.PROJECTNAME = 'CY2_AD_APPL_001' 
     AND NOT EXISTS(SELECT 'X' 
  FROM PSPROJECTITEM D, PSDBFIELD C
  WHERE D.OBJECTTYPE = 2
     AND D.OBJECTVALUE1 = C.FIELDNAME
     AND D.OBJECTID2 = 0
     AND B.FIELDNAME = C.FIELDNAME
    AND A.PROJECTNAME = D.PROJECTNAME)
    
   /* SQLS IN PEOPLECODE IN PROJECT THAT DO NOT EXIST IN THE SQL DEFN TABLE */
   SELECT DISTINCT B.REFNAME, A.OBJECTVALUE1, A.OBJECTVALUE2, A.OBJECTVALUE3, A.OBJECTVALUE4
  FROM PSPROJECTITEM A, PSPCMNAME B
  WHERE A.OBJECTVALUE1 = B.OBJECTVALUE1
     AND A.OBJECTVALUE2 = B.OBJECTVALUE2
     AND ( A.OBJECTVALUE3 = B.OBJECTVALUE3
     OR A.OBJECTVALUE3 = ' ')
     AND ( A.OBJECTVALUE4 = B.OBJECTVALUE4
     OR A.OBJECTVALUE4 = ' ')
     AND A.OBJECTID1 = B.OBJECTID1
     AND A.OBJECTID2 = B.OBJECTID2
     AND ( A.OBJECTID3 = B.OBJECTID3
     OR A.OBJECTID3 = 0)
     AND ( A.OBJECTID4 = B.OBJECTID4
     OR A.OBJECTID4 = 0)
   AND B.RECNAME= 'SQL'
   AND A.PROJECTNAME = 'CY2_AD_APPL_001'
   AND NOT EXISTS(SELECT 'X' FROM PSSQLTEXTDEFN C WHERE B.REFNAME = C.SQLID);
   
   
   /* MESSAGE CATALOG ENTRIES IN THE PROJECT THAT ARE USED IN PEOPLECODE BASED ON OBECT TYPE */
   SELECT distinct B.PROJECTNAME, A.MESSAGE_SET_NBR, A.MESSAGE_NBR, C.OBJECTVALUE1, c.OBJECTVALUE2, c.OBJECTVALUE3, c.OBJECTVALUE4, c.OBJECTVALUE5
  FROM PSMSGCATDEFN A, PSPCMTXT C, PSPROJECTITEM B, PSPCMPROG D
  WHERE  B.OBJECTTYPE = 25
     AND B.OBJECTID1 = 48
     AND B.OBJECTVALUE1 = A.MESSAGE_SET_NBR
     AND B.OBJECTID2 = 49
     AND B.OBJECTVALUE2 = A.MESSAGE_NBR
     AND B.PROJECTNAME = 'CY2_AD_APPL_001' 
AND D.OBJECTID1 = C.OBJECTID1 
   AND D.OBJECTVALUE1 = C.OBJECTVALUE1 
   AND D.OBJECTID2 = C.OBJECTID2 
   AND D.OBJECTVALUE2 = C.OBJECTVALUE2 
   AND D.OBJECTID3 = C.OBJECTID3 
   AND D.OBJECTVALUE3 = C.OBJECTVALUE3 
   AND D.OBJECTID4 = C.OBJECTID4 
   AND D.OBJECTVALUE4 = C.OBJECTVALUE4 
   AND D.OBJECTID5 = C.OBJECTID5 
   AND D.OBJECTVALUE5 = C.OBJECTVALUE5 
   AND D.OBJECTID6 = C.OBJECTID6 
   AND D.OBJECTVALUE6 = C.OBJECTVALUE6 
   AND D.OBJECTID7 = C.OBJECTID7 
   AND D.OBJECTVALUE7 = C.OBJECTVALUE7 
   AND D.PROGSEQ = C.PROGSEQ
and c.OBJECTVALUE1 like 'CY2%' 
 AND C.PCTEXT LIKE ('%' || A.MESSAGE_SET_NBR || ', ' || A.MESSAGE_NBR || '%');
 
 
 /* MESSAGE CATALOG ENTRIES IN PAGE FIELDS THE PROJECT */
 SELECT c.projectname, A.PNLNAME, A.GRDLBLMSGSET, A.GRDLBLMSGnum
  FROM PSPNLFIELD A, PSPNLDEFN B, PSPROJECTITEM C
  WHERE  A.PNLNAME = B.PNLNAME
     AND A.PNLNAME = C.OBJECTVALUE1
     AND C.OBJECTTYPE = 5
      AND C.PROJECTNAME in( 'CY2_LOG_16', 'CY2_LOG_INTEGRATION_001')
  AND A.GRDLBLMSGSET > 0;
  
 
   /* MESSAGE CATALOG ENTRIES IN THE PROJECT THAT ARE USED IN PEOPLECODE BASED ON LASTUPDPRID <> 'PPLSOFT' */
   SELECT distinct B.PROJECTNAME, A.MESSAGE_SET_NBR, A.MESSAGE_NBR, C.OBJECTVALUE1, c.OBJECTVALUE2, c.OBJECTVALUE3, c.OBJECTVALUE4, c.OBJECTVALUE5
  FROM PSMSGCATDEFN A, PSPCMTXT C, PSPROJECTITEM B, PSPCMPROG D
  WHERE  B.OBJECTTYPE = 25
     AND B.OBJECTID1 = 48
     AND B.OBJECTVALUE1 = A.MESSAGE_SET_NBR
     AND B.OBJECTID2 = 49
     AND B.OBJECTVALUE2 = A.MESSAGE_NBR
     AND B.PROJECTNAME = 'CY2_LOG_INTEGRATION_001' 
AND D.OBJECTID1 = C.OBJECTID1 
   AND D.OBJECTVALUE1 = C.OBJECTVALUE1 
   AND D.OBJECTID2 = C.OBJECTID2 
   AND D.OBJECTVALUE2 = C.OBJECTVALUE2 
   AND D.OBJECTID3 = C.OBJECTID3 
   AND D.OBJECTVALUE3 = C.OBJECTVALUE3 
   AND D.OBJECTID4 = C.OBJECTID4 
   AND D.OBJECTVALUE4 = C.OBJECTVALUE4 
   AND D.OBJECTID5 = C.OBJECTID5 
   AND D.OBJECTVALUE5 = C.OBJECTVALUE5 
   AND D.OBJECTID6 = C.OBJECTID6 
   AND D.OBJECTVALUE6 = C.OBJECTVALUE6 
   AND D.OBJECTID7 = C.OBJECTID7 
   AND D.OBJECTVALUE7 = C.OBJECTVALUE7 
   AND D.PROGSEQ = C.PROGSEQ
   AND D.LASTUPDOPRID <> 'PPLSOFT' 
 AND C.PCTEXT LIKE ('%' || A.MESSAGE_SET_NBR || ', ' || A.MESSAGE_NBR || '%')
 
/* message catalog entries in component details */
SELECT * FROM PSPNLGRPDEFN WHERE ADDLINKMSGSET > 20000 OR SRCHLINKMSGSET > 20000 OR  SRCHTEXTMSGSET > 20000
SELECT * FROM PSPNLGRPDEFNEXT WHERE PTKEYWDSRCHMSGSET  > 20000 OR  PTRTSRCHLINKMSGSET > 20000
